      <profiles>
        <profile id="dd86-1074-b1f4-9ff3" name="Parasite Grenades" hidden="false" typeId="3a58-f011-75f9-89dc" typeName="Weapon">
          <characteristics>
            <characteristic name="Type" typeId="884f-9de1-3446-9e6f">Grenade</characteristic>
