    <selectionEntryGroup id="2953-5bc5-47db-7d89" name="Secrets of the House of Wisdom" hidden="false" collective="false" import="true">
      <constraints>
        <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="33ef-e023-1459-6cdc" type="max"/>
