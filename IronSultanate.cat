            <constraint field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="97f4-8822-bc73-4442" type="max"/>
          </constraints>
          <rules>
            <rule id="2938-e05e-ae50-fc12" name="Mirage of Time" hidden="false">
              <description>Both Ranged and Melee attacks against the Assassin suffer -1 DICE penalty when determining if they hit.</description>
            </rule>
          </rules>
