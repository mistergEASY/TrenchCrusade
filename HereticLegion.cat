  <forceEntries>
    <forceEntry id="a299-4c55-fb3a-3df2" name="Naval Raiding Party" hidden="false">
      <rules>
        <rule id="e32f-aae4-5b93-ed4e" name="Fast as Lightning" hidden="false">
          <description>All Models have +1 DICE when taking their Dash ACTIONS.</description>
        </rule>
        <rule id="6ad3-0b4a-1ef9-0fa3" name="Close Assault Weapons" hidden="false">
          <description>The warband can buy Submachine Guns for 25 ducats per weapon.</description>
        </rule>
        <rule id="8a34-5a84-4b81-fe85" name="Light Troops" hidden="false">
          <description>The force may only include 0-1 Annointed and 0-1 Artillery Witch.</description>
        </rule>
        <rule id="e2d2-ff6c-7bec-04c2" name="Let Sleeping Dogs Lie" hidden="false">
          <description>The warband may not include a War Wolf.</description>
        </rule>
        <rule id="3a65-ada5-07fc-84d5" name="Unseen Advance" hidden="false">
          <description>Up to three models without ELITE Keyword can be upgraded into INFILTRATORS at the cost of 10 ducats per Model.</description>
        </rule>
      </rules>
      <categoryLinks>
        <categoryLink id="de6c-c1e9-0e6e-9a4c" name="Leader" hidden="false" targetId="fd03-b929-b7d2-fa9a" primary="false"/>
